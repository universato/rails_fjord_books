<p id="notice"><%= notice %></p>

<h1>ユーザー一覧</h1>

<br><br>

<table>
  <thead>
    <tr>
      <th><%= User.human_attribute_name(:email) %></th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= link_to t(:show, scope: %i[dictionary button]), user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

